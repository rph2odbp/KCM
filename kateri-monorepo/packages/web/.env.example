# Firebase Configuration
# For local emulator: use non-empty placeholders below. The Auth emulator ignores the value but the SDK requires it.
VITE_FIREBASE_API_KEY=fake-api-key
VITE_FIREBASE_AUTH_DOMAIN=localhost
VITE_FIREBASE_PROJECT_ID=demo-project
VITE_FIREBASE_STORAGE_BUCKET=demo-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_FIREBASE_FUNCTIONS_REGION=us-central1

# Environment
VITE_APP_ENV=development
VITE_USE_EMULATORS=true

# Observability (Sentry)
VITE_ENABLE_SENTRY=false
VITE_SENTRY_DSN=
VITE_SENTRY_ENV=development
VITE_SENTRY_TRACES_SAMPLE_RATE=0
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1

# API URLs (when not using Firebase emulators)
VITE_API_BASE_URL=http://localhost:5001/your_project/us-central1